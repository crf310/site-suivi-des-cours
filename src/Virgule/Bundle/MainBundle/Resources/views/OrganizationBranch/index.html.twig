{% extends '::base.html.twig' %}
{% block body %}
<h1>OrganizationBranch list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Presidentname</th>
            <th>Phonenumber</th>
            <th>Faxnumber</th>
            <th>Emailaddress</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('organizationbranch_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.address }}</td>
            <td>{{ entity.presidentName }}</td>
            <td>{{ entity.phoneNumber }}</td>
            <td>{{ entity.faxNumber }}</td>
            <td>{{ entity.emailAddress }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('organizationbranch_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('organizationbranch_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('organizationbranch_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}